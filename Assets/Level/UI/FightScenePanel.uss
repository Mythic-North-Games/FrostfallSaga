}

.TimelineCharacter:hover {
    background-color: rgba(240, 240, 240, 0);
    background-image: url("project://database/Assets/Art/UI/CharacterParts/CharBackground.png?fileID=2800000&guid=bc4a83db463c6254e9042aec6229f409&type=3#CharBackground");

.SettingBouton {
    background-image: url("project://database/Assets/Art/UI/sliders/SliderArrow.png?fileID=2800000&guid=9ee871b18a0bae64eb4872f6fbf24bd9&type=3#SliderArrow");
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(188, 188, 188, 0);
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    margin-left: 0;
    margin-bottom: 0;
    margin-right: 0;
    margin-top: 0;
    -unity-background-scale-mode: stretch-to-fill;
    width: 100%;
    height: 80%;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.SettingBouton:hover {
    background-image: url("project://database/Assets/Art/UI/sliders/SliderArrow.png?fileID=2800000&guid=9ee871b18a0bae64eb4872f6fbf24bd9&type=3#SliderArrow");
    -unity-background-image-tint-color: rgb(221, 55, 55);
    background-color: rgba(221, 55, 55, 0);
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    margin-left: 0;
    margin-bottom: 0;
    margin-right: 0;
    margin-top: 0;
    -unity-background-scale-mode: stretch-to-fill;
    right: auto;
    transition-timing-function: ease, ease;
    transition-delay: 0s, 0s;
    transition-duration: 0.15s, 0s;
}

.SettingBouton:hover#SettingSharpIcon {
    background-image: url("project://database/Assets/Art/UI/icon_global/Settings_sharp_icon.png?fileID=2800000&guid=bbd786871a733a348bc6815b76d8011e&type=3#Settings_sharp_icon");
    -unity-background-image-tint-color: rgb(249, 0, 0);
    background-color: rgba(224, 28, 28, 0);
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
    margin-left: 0;
    margin-bottom: 0;
    margin-right: 0;
    margin-top: 0;
    -unity-background-scale-mode: stretch-to-fill;
    right: auto;
    scale: 1 1;
    transform-origin: left;
    translate: -45% 0;
    transition-timing-function: ease, ease;
    transition-delay: 0s, 0s;
    transition-duration: 0.5s, 0s;
}

.Setting_Sharp_Icon {
    flex-grow: 1;
    background-image: url("project://database/Assets/Art/UI/icon_global/Settings_sharp_icon.png?fileID=2800000&guid=bbd786871a733a348bc6815b76d8011e&type=3#Settings_sharp_icon");

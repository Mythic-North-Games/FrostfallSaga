    transition-timing-function: ease-in-out-cubic;
}

.itemSlotContainerActive {
    width: 100%;
    height: 100%;
    background-image: url("project://database/Assets/Art/UI/Containers/handDrawnSelectedSquareContainer.png?fileID=2800000&guid=aec95d218e91933b38e63f73f5ba4688&type=3#handDrawnSelectedSquareContainer");
    -unity-background-image-tint-color: rgb(25, 25, 25);
    padding-top: 10%;
    padding-right: 10%;
    padding-bottom: 10%;
    padding-left: 10%;
    transition-duration: 0.25s;
    transition-timing-function: ease-in-out-cubic;
    scale: 1 1;
}

.itemSlotContainerSelected {
    position: absolute;
    width: 100%;
    height: 100%;
    background-image: url("project://database/Assets/Art/UI/Containers/handDrawnSelectedSquareContainer.png?fileID=2800000&guid=aec95d218e91933b38e63f73f5ba4688&type=3#handDrawnSelectedSquareContainer");
    -unity-background-image-tint-color: rgb(25, 25, 25);
    opacity: 0;
    transition-duration: 0.25s;
    transition-timing-function: ease-in-out-cubic;
    top: 0;
    left: 0;
}

.itemDetailsContainerRootDefault {

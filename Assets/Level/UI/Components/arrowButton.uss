.leftArrow {
    flex-grow: 0;
    position: relative;
    width: 25px;
    height: 30px;
    background-image: resource('UI/Icons/handDrawnArrowIcon');
    -unity-background-image-tint-color: rgb(25, 25, 25);
    -unity-background-scale-mode: scale-to-fit;
    transition-duration: 0.5s;
    transition-timing-function: ease-in-out;
    left: -45px;
}

.leftArrowLeft {
    flex-grow: 0;

.controlPanelButton {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-left: 0;
    width: auto;
    -unity-font-definition: url("project://database/Assets/Level/UI/FontAssets/JimNightshade-Regular%20SDF.asset?fileID=11400000&guid=c1721a64485c07a1b951d49b9c4cd202&type=2#JimNightshade-Regular SDF");
    font-size: 350%;
    border-left-color: rgba(25, 25, 25, 0.78);
    border-right-color: rgba(25, 25, 25, 0.78);
    border-top-color: rgba(25, 25, 25, 0.78);
    border-bottom-color: rgba(25, 25, 25, 0.78);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    color: rgb(25, 25, 25);
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    -unity-text-align: middle-center;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-cubic;
    transition-property: all;
    align-items: center;
    justify-content: center;
    padding-bottom: 0;
    -unity-font-style: normal;
    -unity-text-outline-width: 1px;
    -unity-text-outline-color: rgb(25, 25, 25);
    flex-grow: 1;
    background-color: rgba(0, 0, 0, 0);
}

